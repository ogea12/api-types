{
  "name": "@ogea12/api-types",
  "description": "Description des types utilisÃ©s dans l'API de l'OGEA 12",
  "version": "1.3.1",
  "engines": {
    "node": ">=20.17.0"
  },
  "type": "module",
  "files": [
    "types/api.d.ts"
  ],
  "exports": {
    ".": "./types/api.d.ts"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "generate": "openapi-typescript openapi.json -o types/api.d.ts --root-types",
    "release": "release-it",
    "taze": "taze"
  },
  "homepage": "https://github.com/ogea12/api-types#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogea12/api-types.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@ogea12/commitlint-config": "^1.0.6",
    "@ogea12/eslint-config": "^2.1.0",
    "@ogea12/prettier-config": "^1.0.9",
    "@ogea12/tsconfig": "^1.0.12",
    "@swc/core": "^1.15.11",
    "@types/node": "^22.19.7",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "release-it": "^19.2.4",
    "taze": "^19.9.2",
    "typescript": "^5.8.3"
  },
  "author": "OGEA 12",
  "license": "MIT",
  "keywords": [
    "ogea12",
    "api",
    "openapi"
  ],
  "prettier": "@ogea12/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
